PROG    =       main
SRCS    =       hash_table.c    \
                main.c
OBJS    =       $(SRCS:.c=.o)
DEPS    =       hash_table.h
CC      =       gcc
CFLAGS  =       -g -std=gnu11 -Wall -I

$(PROG) : $(SRCS) $(DEPS)
        $(CC) $(CFLAGS) -o $(PROG) $(SRCS)

clean :
        /bin/rm -f $(OBJS) $(PROG)
